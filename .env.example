# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role (para operações do servidor)
# Obtenha no dashboard do Supabase > Settings > API > service_role key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# VAPID Keys (gere com: npx web-push generate-vapid-keys)
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-public-key
VAPID_PRIVATE_KEY=your-vapid-private-key

# Admin
ADMIN_PASSWORD=your-admin-password

# Cron Secret (para autenticar chamadas do cron job da Vercel)
CRON_SECRET=your-cron-secret
